{
    "name": "falai-chat",
    "version": "0.1.0",
    "author": "laruss5",
    "description": "A Next.js 15 chat application for generating AI images using fal.ai models with AI SDK 5.0",
    "keywords": [
        "fal.ai",
        "ai",
        "image-generation",
        "nextjs",
        "chat",
        "ai-sdk",
        "flux",
        "stable-diffusion"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/laruss5/falai-chat"
    },
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "bun --bun next dev --turbopack",
        "build": "bun --bun next build --turbopack",
        "start": "next start",
        "lint": "bunx eslint src --ext .ts,.js,.mjs",
        "lint:fix": "bunx eslint src --ext .ts,.js,.mjs --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "typecheck": "tsc --noEmit",
        "check:fix": "bun run lint:fix && bun run format && bun run typecheck"
    },
    "dependencies": {
        "@ai-sdk/fal": "^1.0.15",
        "@ai-sdk/react": "^2.0.57",
        "@mynaui/icons-react": "^0.3.9",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-scroll-area": "^1.2.10",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tooltip": "^1.2.8",
        "ai": "^5.0.57",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "embla-carousel-react": "^8.6.0",
        "lucide-react": "^0.544.0",
        "next": "15.5.4",
        "next-themes": "^0.4.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.3.1",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/bun": "^1.2.23",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9.30.0",
        "eslint-config-next": "^15.3.4",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "prettier": "3.6.2",
        "tailwindcss": "^4",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5"
    }
}
